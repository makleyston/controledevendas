<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controledevendasifce.controller.FXMLDocumentController">
  <children>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="Cadastros">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#hanlerCadastroCliente" text="Clientes" fx:id="menuItemCadastrosClientes" />
            <MenuItem mnemonicParsing="false" text="Produtos" fx:id="menuItemCadastrosProdutos" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Vendas">
          <items>
            <MenuItem mnemonicParsing="false" text="Realizar Vendas" fx:id="menuItemVendasRealizarVendas" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Relatorios">
          <items>
            <MenuItem mnemonicParsing="false" text="Vendas por mes" fx:id="menuItemRelatoriosVendasPorMes" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="anchorPane" prefHeight="576.0" prefWidth="800.0" />
  </children>
  <stylesheets>
    <URL value="@fxmldefault.css" />
  </stylesheets>
</VBox>
